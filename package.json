{
  "name": "pokekit-ui",
  "version": "0.1.9",
  "description": "Vue 3 Pokemon-themed UI components with PrimeVue and Tailwind CSS",
  "type": "module",
  "main": "./dist/pokekit-ui.umd.cjs",
  "module": "./dist/pokekit-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/pokekit-ui.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/pokekit-ui.umd.cjs"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build-lib": "vite build --mode lib && vue-tsc -p tsconfig.lib.json && node -e \"require('fs').copyFileSync('dist/index.d.ts', 'dist/index.d.mts')\"",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build-lib"
  },
  "keywords": [
    "vue",
    "vue3",
    "pokemon",
    "ui",
    "components",
    "primevue",
    "tailwind",
    "typescript"
  ],
  "author": "klay",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Klay376014/pokekit-ui.git"
  },
  "homepage": "https://github.com/Klay376014/pokekit-ui",
  "peerDependencies": {
    "vue": "^3.5.0",
    "primevue": "^4.0.0",
    "primeicons": "^7.0.0",
    "@primeuix/themes": "^1.0.0"
  },
  "dependencies": {
    "@primeuix/styled": "^0.7.2"
  },
  "devDependencies": {
    "@types/node": "^24.5.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "tailwindcss-primeui": "^0.6.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5",
    "vitest": "^3.2.4",
    "@vitest/browser": "^3.2.4",
    "playwright": "^1.55.0",
    "@vitest/coverage-v8": "^3.2.4"
  }
}
